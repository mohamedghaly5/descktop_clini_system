{
  "name": "dental-flow",
  "productName": "Dental Flow",
  "version": "1.0.7",
  "description": "نظام إدارة عيادة الأسنان",
  "author": {
    "name": "Mohamed Ghaly",
    "email": "mohamedghaly5@github.com"
  },
  "private": true,
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"vite --host\" \"wait-on tcp:8080 && tsc -b electron && tsc -p electron/tsconfig.preload.json && electron .\"",
    "start": "concurrently \"vite preview --port 8080\" \"wait-on tcp:8080 && tsc -b electron && tsc -p electron/tsconfig.preload.json && electron .\"",
    "start-client": "tsc -b electron && electron . --user-data-dir=./client-data",
    "build": "tsc -b electron && tsc -p electron/tsconfig.preload.json && vite build && electron-builder",
    "deploy": "electron-builder --win --publish always",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mohamedghaly5/descktop_clini_system.git"
  },
  "build": {
    "asar": false,
    "appId": "com.ghaly.dentalapp",
    "directories": {
      "output": "release"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "mohamedghaly5",
        "repo": "descktop_clini_system"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "nsis": {
      "include": "build/installer.nsh"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": ".env.production",
        "to": ".env.production"
      }
    ],
    "extends": null
  },
  "dependencies": {
    "@capacitor/android": "^8.0.2",
